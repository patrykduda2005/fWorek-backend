    <script>
        window.addEventListener('load', function () {
            var respectPolicyAction = "";
            var cookieSettingsAction = "";

            var documentUrlAction = document.URL.split("/").pop();
            if (documentUrlAction == respectPolicyAction || documentUrlAction == cookieSettingsAction) {
                var divClose = document.getElementById('divCloseFrame');
                var btnClose = document.getElementById('btnCloseFrame');
                divClose.style.display = 'block';
                btnClose.addEventListener('click', function () {
                    if (window.parent) {
                        window.parent.postMessage({ msg: 'close-iframe' });
                    }
                });
            }
        });
    </script>
